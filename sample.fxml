<?language javascript?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <Label text="Введите диапазон" >
    </Label>
    <Label text="От:" GridPane.rowIndex="1">
    </Label>


    <TextField fx:id="input" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <Label text="До:" GridPane.rowIndex="2">
    </Label>
    <TextField fx:id="input2" GridPane.columnIndex="1" GridPane.rowIndex="2" />
    <Button fx:id="button" layoutX="39.0" layoutY="90.0" mnemonicParsing="false" onAction="#onClickMethod"
            prefHeight="43.5" prefWidth="200.0" GridPane.rowIndex="3" text="Вывести 5 случайных чисел"/>
    <Label fx:id="result" text=" " GridPane.rowIndex="4">
    </Label>
    <Label fx:id="error" text=" " GridPane.rowIndex="5">

    </Label>

</GridPane>